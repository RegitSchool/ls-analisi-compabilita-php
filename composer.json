{
    "name": "scaforchio/lampschool",
    "type": "project",
    "description": "Registro Elettronico per il sistema scolastico italiano.",
    "keywords": [
        "Registro Elettronico",
        "Italian Linux Society"
    ],
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Pietro Tamburrano",
            "role": "Author"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/scaforchio/LAMPSchool/graphs/contributors",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "datatables.net/datatables.net": "dev-master",
        "datatables.net/datatables.net-bs5": "dev-master",
        "datatables.net/datatables.net-buttons": "dev-master",
        "datatables.net/datatables.net-buttons-bs5": "dev-master",
        "datatables.net/datatables.net-keytable": "dev-master",
        "datatables.net/datatables.net-keytable-bs5": "dev-master",
        "datatables.net/datatables.net-responsive": "dev-master",
        "datatables.net/datatables.net-responsive-bs5": "dev-master",
        "php-mqtt/client": "^1.8",
        "picqer/php-barcode-generator": "^2.4",
        "spomky-labs/otphp": "^10.0",
        "twbs/bootstrap": "^5.3",
        "twbs/bootstrap-icons": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "LampSchool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LampSchool\\Tests\\": "tests/"
        }
    }
}
